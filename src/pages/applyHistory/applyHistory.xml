<view class="container">
  <view class="applyHistory-no" wx:if="{{apply}}">
    <view class="applyHistory"></view>
    <view class="applyHistory-txt">
      <text>健健康康比什么都重要噢！</text>
    </view>
  </view>
  <view class="applyHistory-list" wx:else>
    <view class="applyHistory-item" catchtap="linkTo" data-link="/pages/applyDetail/applyDetail?type={{item.id}}" wx:for="{{array}}" wx:for-index="idx">
      <text class="applyHistory-item-txt">{{item.projectName}}</text>
      <text class="applyHistory-item-cont">{{item.claimContent}}</text>
      <text class="applyHistory-item-time">{{item.claimTimeStr}}</text>
    </view>
  </view>
  
</view>
