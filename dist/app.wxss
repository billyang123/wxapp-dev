  font-size: 32rpx;
}
.gray {
  color: #999;
}
.gray-m {
  color: #666;
}
.tel-btn {
  display: inline-block;
  border: 2px solid #ff6a00;
  border-radius: 4rpx;
  width: 342rpx;
  height: 61rpx;
  font-size: 32rpx;
  color: #ff6a00;
  line-height: 55rpx;
  background-color: transparent;
}
.default-image {
  background: url("/images/default-image.png") no-repeat center #dddee3;
  width: 100%;
  height: 100%;
  background-position: center center;
  background-size: 60%;
}
