.navtab {
  overflow: hidden;
  width: 750rpx;
}
.navtab .tab-item {
  width: 374rpx;
  float: left;
  background-color: #fff;
  border-left: 1rpx solid #ddd;
  text-align: center;
  font-size: 36rpx;
  border-bottom: 1rpx solid #ddd;
}
.navtab .tab-item.active {
  color: #ff6a00;
}
.navtab .tab-item.active .tab-link {
  border-bottom: 5rpx solid #ff6a00;
}
.navtab .tab-item:first-child {
  border-left: 0;
}
.navtab .tab-link {
  display: block;
  padding: 35rpx 0  35rpx 0;
  border-bottom: 5rpx solid transparent;
}
.publicity-item-more {
  text-align: center;
  padding: 50rpx 0;
}
.publicity-item-more .txt {
  margin-top: 40rpx;
  display: block;
  text-align: center;
}
.publicity-item-more .more-link {
  color: #ff6a00;
  margin-top: 30rpx;
}
.fundsPublicity-item {
  background-color: #fff;
  padding: 38rpx 35rpx;
  margin-bottom: 10rpx;
}
.fundsPublicity-item-inner {
  background-image: url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAA0AAAAXBAMAAAAiiNiyAAAAG1BMVEUAAACrq6uqqqqrq6uqqqqrq6urq6vMzMyqqqqiVq4uAAAACHRSTlMAoeWJinDgBVRF68UAAAAwSURBVAjXYwgyYAADjUYI7dEhAKYZO6ACEuQIQAwt61CA8WHyCPWEuQj3INwHcy8A8hQTRXGkcZ8AAAAASUVORK5CYII=);
  background-position: right center;
  background-repeat: no-repeat;
  background-size: 13rpx 23rpx;
  padding-right: 30rpx;
  overflow: hidden;
}
.fundsPublicity-item .dt {
  float: left;
  width: 180rpx;
  height: 180rpx;
  text-align: center;
  background-color: #FF6A00;
}
.fundsPublicity-item .dt .p {
  padding-top: 30rpx;
  font-size: 60rpx;
  line-height: 70rpx;
  color: #fff;
}
.fundsPublicity-item .dt .i {
  color: #fff;
  font-size: 28rpx;
}
.fundsPublicity-item .dd {
  float: left;
  width: 410rpx;
  padding-left: 28rpx;
}
.fundsPublicity-item .dd .h2 {
  padding-top: 5rpx;
  font-size: 36rpx;
  margin-bottom: 20rpx;
}
.fundsPublicity-item .dd .p {
  font-size: 28rpx;
  line-height: 40rpx;
}
.casePublicity-item {
  padding: 40rpx 36rpx;
  background-color: #fff;
}
.casePublicity-item .dl .dt {
  float: left;
  width: 200rpx;
  height: 200rpx;
}
.casePublicity-item .dl .dd {
  float: left;
  padding-left: 28rpx;
}
.casePublicity-item .dl .dd .h2 {
  font-size: 36rpx;
  margin-bottom: 25rpx;
}
.casePublicity-item .dl .dd .p {
  font-size: 28rpx;
  color: #666;
  line-height: 44rpx;
}
.casePublicity-item .dl .dd .p .t {
  color: #999;
}
.casePublicity-item .dl .dd .p .org {
  color: #ff6a00;
}
.casePublicity-item-inner .dl {
  overflow: hidden;
}
.casePublicity-item .bt-dl {
  padding: 30rpx 35rpx;
}
