<view class="container elecert">
<view class="card">
<view class="card-title">电子凭证</view>
<view class="card-content">
<view class="p">
<text class="t">社群名称：</text>
<text class="gray-m">{{project.projectName}}</text>
</view>
<view class="p">
<text class="t">互助金额：</text>
<text class="gray-m">最高{{project.projectGuaranteeMax}}元</text>
</view>
<view class="p mt">
<text class="t">姓名：</text>
<text class="gray-m">{{projectAccount.insuredName}}</text>
</view>
<view class="p" xmlns:wx="" wx:if="{{projectAccount.relationshipCode}}">
<text class="t">关系：</text>
<text class="gray-m">{{relationship[projectAccount.relationshipCode]}}</text>
</view>
<view class="p" wx:else="">
<text class="t">身份证号：</text>
<text class="gray-m">{{projectAccount.insuredIdNumber}}</text>
</view>
<view class="p mt">
<text class="t">加入时间：</text>
<text class="gray-m">{{projectAccount.createTime}}</text>
</view>
<view class="p">
<text class="t">生效时间：</text>
<text class="gray-m">{{projectAccount.effectiveTime}}</text>
</view>
</view>
</view>
<view class="card-info media-list">
<navigator class="item-content" url="/pages/range/range?type={{projectId}}">
<view class="item-inner item-link-inner">
<view class="item-title">互助范围</view>
</view>
</navigator>
<view class="item-content" wx:for="{{list}}" wx:key="idx" wx:for-index="idx">
<view class="item-inner">
<view class="item-title-row">
<view class="item-title">{{item.subject_name}}</view>
<view class="item-after" wx:if="{{item.subject_status == 2}}">
<text class="blue">保障中</text>
</view>
<view class="item-after" wx:if="{{item.subject_status == 1}}">
<text class="gray-m">等待期还剩{{item.subject_waite_time}}天</text><icon class="iconwh" bindtap="_dispatch" data-bindtap="showIntro"/>
</view>
</view>
<view class="item-subtitle">{{item.subject_desc}}</view>
</view>
</view>
</view>
<view style="padding:10rpx 34rpx">
<view class="fz28">
<text class="gray">已同意</text><text>{{items[0]}}</text>
</view>
<view class="fz28">
<text class="gray">已同意</text><text>{{items[projectId]}}</text>
</view>
</view>
<view class="btn-padd">
<view class="btn btn-org" catchtap="_dispatch" data-link="/pages/community/community?type={{projectId}}" data-catchtap="linkTo">
<text>查看社群详情</text>
</view>
</view>
</view>