.add-btn {
  border: 1rpx solid #ddd;
  border-width: 1rpx;
  border-radius: 5rpx;
  width: 449rpx;
  height: 86rpx;
  line-height: 86rpx;
  font-size: 36rpx;
  color: #333;
  text-align: center;
  margin: 40rpx auto;
  background: #f8f8f8;
}
.input-area {
  margin-top: 20rpx;
  background-color: #fff;
  overflow: hidden;
  position: relative;
  margin-bottom: 20rpx;
  box-shadow: 0 1px 0 0 #ddd;
}
.input-area .section {
  padding: 26rpx 35rpx;
  overflow: hidden;
  border-bottom: 1rpx solid rgba(221, 221, 221, 0.5);
}
.input-area .section:last-child {
  border-bottom: 0;
}
.input-area .section_title {
  font-size: 34rpx;
  width: 200rpx;
  float: left;
  height: 52rpx;
  line-height: 52rpx;
}
.input-area input {
  height: 52rpx;
  width: 335rpx;
  float: left;
}
.input-area input::-webkit-input-placeholder {
  font-size: 24rpx;
  color: #c7c7c7;
}
.input-area .sec-con {
  float: left;
  width: 625rpx;
}
.del-btn {
  float: left;
  width: 120rpx;
  height: 163rpx;
  margin-top: 20rpx;
  border-left: 1rpx solid #ddd;
}
.del-icon {
  position: absolute;
  width: 64rpx;
  height: 64rpx;
  background-image: url(data:image/png;base64,);
  background-size: 64rpx 64rpx;
  background-repeat: no-repeat;
  right: 30rpx;
  top: 50%;
  margin-top: -32rpx;
}
.intro {
  font-size: 28rpx;
  color: #999;
  padding: 0 35rpx;
  line-height: 40rpx;
}
.intro .tip {
  margin-top: 20rpx;
}
.money {
  padding: 0 35rpx;
  margin-bottom: 20rpx;
}
.money .t {
  height: 40rpx;
  border-left: 10rpx solid #ff6a00;
  text-indent: 20rpx;
  font-size: 36rpx;
  color: #333;
  line-height: 40rpx;
  margin-bottom: 30rpx;
}
.money .num {
  background: url(https://image1.chinamuxie.com/images/common/money_icon.png) no-repeat;
  background-size: 226rpx 84rpx;
  border-radius: 5rpx;
  display: inline-block;
  padding: 25rpx 88rpx;
  color: #ff6a00;
  font-size: 36rpx;
}
.checkbox-group {
  padding: 0 35rpx;
}
.checkbox-group .checkbox {
  font-size: 28rpx;
  color: #666;
  line-height: 40rpx;
  margin-bottom: 20rpx;
}
.checkbox-group .checkbox checkbox {
  position: relative;
  top: 4rpx;
}
.justjoin {
  position: fixed;
  bottom: 0;
  text-align: center;
  background: #ff6a00;
  width: 750rpx;
  height: 98rpx;
  color: #fff;
  line-height: 98rpx;
  font-size: 36rpx;
}
