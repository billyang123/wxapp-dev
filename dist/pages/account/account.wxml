<view class="container">
<!-- <component key="navbar"/> -->
<view class="account-header">
<view class="account-info login" xmlns:wx="" wx:if="{{login}}">
<navigator class="acimage default-image" url="/pages/profile/profile">
<image src="{{userInfo.headImgUrl || wxUserInfo.avatarUrl}}" class="avatar"/>
<view class="volunteer" wx:if="{{isVolunteer}}">
<text>自愿者</text>
</view>
</navigator>
<view class="txt-area" wx:if="{{userInfo.existGuarantee}}">
<view class="name">
<text>{{userInfo.nickName}}</text>
</view>
<view class="hnumber">
<view class="hn-l">
<view class="num"><text>第{{userInfo.serialNumber}}名加入社群，已加入{{userInfo.joinedDays}}天</text></view>
</view>
</view>
<!-- <view class="msgnum-icon">
<view class="num">
<text>2</text>
</view>
</view> -->
</view>
<view class="txt" wx:else="wx:else">
<text>{{userInfo.nickName}}</text>
</view>
</view>
<view wx:else="wx:else" class="account-info no-login">
<view class="acimage default-image" catchtap="_dispatch" data-catchtap="bindLogin"/>
<view class="txt" catchtap="_dispatch" data-catchtap="bindLogin">
<text>立即登录</text>
</view>
</view>
</view>
<view class="media-list ac-list">
<view class="item-content" catchtap="_dispatch" data-link="/pages/inification/inification" data-catchtap="linkTo">
<view class="item-media">
<image class="ac-list-img" src="{{assetsPath}}/images/account_wdsq_icon.png"/>
</view>
<view class="item-inner item-link-inner">
<view class="item-title">
<text>我的互助</text>
</view>
<view class="item-after">
<text>充值、互助凭单</text>
</view>
</view>
</view>
<navigator class="item-content" redirect="" url="/pages/applyhuzhu/applyhuzhu">
<view class="item-media">
<image class="ac-list-img" src="{{assetsPath}}/images/account_sqhz_icon.png"/>
</view>
<view class="item-inner item-link-inner">
<view class="item-title">
<text>申请互助</text>
</view>
</view>
</navigator>
</view>
<view class="media-list ac-list">
<navigator class="item-content" redirect="" url="/pages/qa/qa">
<view class="item-media">
<image class="ac-list-img" src="{{assetsPath}}/images/account-question-icon.png"/>
</view>
<view class="item-inner item-link-inner">
<view class="item-title">
<text>常见问题</text>
</view>
</view>
</navigator>
</view>
<view class="or-line">
<text class="or-line-txt gray">联系我们</text>
</view>
<view class="hzo-service">
<view class="hzs-con">
<view class="item tel">
<text class="gray-m">客服电话：</text>
<button class="tel-btn" data-phone-number="400-688-6259" bindtap="_dispatch" data-bindtap="makePhoneCall">400-688-6259</button>
</view>
<view class="item work-time">
<text>微信客服： 搜索微信号</text> <text class="org">  kefu17hz2</text>
</view>
</view>
</view>
</view>