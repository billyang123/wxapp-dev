.tabBottombar {
  position: fixed;
  bottom: 0;
  left: 0;
  display: flex;
  background: #ffffff;
  width: 750rpx;
  height: 100rpx;
  border-top: 1rpx solid rgba(221, 221, 221, 0.8);
  z-index: 99999;
}
.tabBottombar .vb-block {
  flex-shrink: 1;
  text-align: center;
  padding: 16rpx 0 8rpx 0;
  flex: 1;
}
.tabBottombar .icon-img {
  width: 46rpx;
  height: 46rpx;
}
.tabBottombar .txt {
  font-size: 22rpx;
  line-height: 22rpx;
  color: #7f8389;
  margin-top: 5rpx;
}
.container {
  padding-bottom: 100rpx;
}
.account-header {
  background: #ff6a00;
  height: 250rpx;
  padding: 1rpx;
}
.account-info.no-login {
  color: #fff;
  overflow: hidden;
  margin: 48rpx 36rpx;
}
.account-info.no-login .acimage {
  overflow: hidden;
}
.account-info .txt {
  float: left;
  line-height: 130rpx;
  margin-left: 29rpx;
  color: #fff;
}
.account-info .avatar {
  width: 130rpx;
  height: 130rpx;
  border-radius: 100%;
}
.account-info .acimage {
  float: left;
  background-color: #dddee3;
  border: 5rpx solid rgba(255, 255, 255, 0.3);
  width: 130rpx;
  height: 130rpx;
  border-radius: 100%;
  position: relative;
}
.account-info .acimage .volunteer {
  position: absolute;
  background: #ffe740;
  border: 3rpx solid rgba(255, 255, 255, 0.3);
  border-radius: 20rpx;
  width: 134rpx;
  height: 40rpx;
  line-height: 46rpx;
  text-align: center;
  font-size: 24rpx;
  color: #ff6a00;
  bottom: -22rpx;
  left: 0;
}
.account-info.login {
  margin: 48rpx 36rpx;
}
.account-info .hnumber {
  overflow: hidden;
}
.account-info .th {
  margin-top: 12rpx;
}
.account-info .name {
  margin-bottom: 15rpx;
}
.account-info .name,
.account-info .th,
.account-info .num {
  color: #fff;
}
.account-info .hn-l {
  float: left;
}
.account-info .txt-area {
  float: left;
  margin-left: 29rpx;
  position: relative;
  width: 500rpx;
  margin-top: 20rpx;
}
.account-info .txt-area .th {
  font-size: 22rpx;
}
.account-info .txt-area .msgnum-icon {
  position: absolute;
  right: 0;
  top: -26rpx;
  background-image: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-size: 47rpx 47rpx;
  width: 47rpx;
  height: 47rpx;
}
.account-info .txt-area .msgnum-icon .num {
  background: #ff302a;
  border: 3rpx solid #fff;
  width: 32rpx;
  height: 32rpx;
  line-height: 32rpx;
  border-radius: 100%;
  text-align: center;
  position: absolute;
  top: -12rpx;
  right: -16rpx;
  font-size: 28rpx;
}
.ac-list.media-list .item-title {
  font-size: 34rpx;
}
.ac-list.media-list .item-after {
  font-size: 32rpx;
  color: #999;
}
.ac-list.media-list .item-content {
  min-height: 100rpx;
}
.ac-list.media-list .item-inner {
  min-height: 100rpx;
}
.ac-list .item-media .img {
  width: 48rpx;
  height: 48rpx;
}
.hzo-service {
  background-color: #fff;
}
.hzo-service .tel {
  overflow: hidden;
  border-bottom: 1rpx solid rgba(221, 221, 221, 0.8);
}
.hzo-service .tel .gray-m {
  float: left;
  line-height: 60rpx;
}
.hzo-service .item {
  padding: 34rpx 32rpx;
}
.or-line .txt {
  width: 150rpx;
  margin-left: -75rpx;
}
