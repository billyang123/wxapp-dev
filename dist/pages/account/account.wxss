.account-header {
  background: #ff6a00;
  width: 750rpx;
  height: 250rpx;
  padding: 1rpx;
}
.account-info.no-login {
  color: #fff;
  overflow: hidden;
  margin: 48rpx 36rpx;
}
.account-info.no-login .txt {
  float: left;
  line-height: 130rpx;
  margin-left: 29rpx;
}
.account-info .avatar {
  width: 130rpx;
  height: 130rpx;
  border-radius: 100%;
}
.account-info .acimage {
  float: left;
  overflow: hidden;
  background-color: #dddee3;
  border: 5rpx solid rgba(255, 255, 255, 0.3);
  width: 130rpx;
  height: 130rpx;
  border-radius: 100%;
}
.account-info.login {
  margin: 48rpx 36rpx;
}
.account-info .hnumber {
  overflow: hidden;
}
.account-info .th {
  margin-top: 12rpx;
}
.account-info .name {
  margin-bottom: 23rpx;
}
.account-info .name,
.account-info .th,
.account-info .num {
  color: #fff;
}
.account-info .hn-l {
  float: left;
  margin-right: 60rpx;
}
.account-info .txt-area {
  float: left;
  margin-left: 29rpx;
  position: relative;
  width: 500rpx;
}
.account-info .txt-area .th {
  font-size: 22rpx;
}
.account-info .txt-area .msgnum-icon {
  position: absolute;
  right: 0;
  top: -26rpx;
  background-image: url(/images/message_icon_47x47.png);
  background-repeat: no-repeat;
  background-size: 47rpx 47rpx;
  width: 47rpx;
  height: 47rpx;
}
.account-info .txt-area .msgnum-icon .num {
  background: #ff302a;
  border: 3rpx solid #fff;
  width: 32rpx;
  height: 32rpx;
  line-height: 32rpx;
  border-radius: 100%;
  text-align: center;
  position: absolute;
  top: -12rpx;
  right: -16rpx;
  font-size: 28rpx;
}
.hdnav {
  background-color: #fff;
  overflow: hidden;
  margin-bottom: 20rpx;
}
.hdnav .txt {
  font-size: 32rpx;
  float: left;
}
.hdnav .myicon-index-1,
.hdnav .myicon-index-2 {
  background-repeat: no-repeat;
  position: relative;
  top: -5rpx;
  float: left;
  margin-right: 20rpx;
}
.hdnav .iconfont {
  margin-left: 8rpx;
  font-size: 48rpx;
  float: right;
  margin-right: 40rpx;
  position: relative;
  top: -5rpx;
}
.hdnav .myicon-index-1 {
  background-image: url(data:image/png;base64,);
  background-size: 55rpx 64rpx;
  width: 55rpx;
  height: 64rpx;
  margin-left: 44rpx;
}
.hdnav .myicon-index-2 {
  background-image: url(data:image/png;base64,);
  background-size: 60rpx 62rpx;
  width: 60rpx;
  height: 62rpx;
  margin-left: 40rpx;
}
.hdnav .col-50 {
  line-height: 50rpx;
  height: 60rpx;
  width: 410rpx;
  text-align: center;
  float: left;
  color: #ff6a00;
  padding: 28rpx 0;
}
.hdnav .col-50.nr {
  width: 340rpx;
}
.media-list {
  margin-bottom: 20rpx;
}
.media-list .item-content,
.media-list .item-media {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.media-list .item-content {
  padding-left: 36rpx;
  padding-right: 36rpx;
  background-color: #fff;
  box-sizing: border-box;
  min-height: 100rpx;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
  justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  border-bottom: 1px solid #ddd;
}
.media-list .item-title {
  font-size: 34rpx;
}
.media-list .item-after {
  font-size: 32rpx;
  color: #999;
}
.media-list .item-media {
  -ms-flex: 0 0 auto;
  -webkit-flex-shrink: 0;
  flex-shrink: 0;
  -webkit-box-lines: single;
  -moz-box-lines: single;
  -webkit-flex-wrap: nowrap;
  flex-wrap: nowrap;
  box-sizing: border-box;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  margin-right: 19rpx;
}
