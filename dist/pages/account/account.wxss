.tabBottombar {
  position: fixed;
  bottom: 0;
  left: 0;
  display: flex;
  background: #ffffff;
  width: 750rpx;
  height: 100rpx;
  border-top: 1rpx solid rgba(221, 221, 221, 0.8);
  z-index: 99999;
}
.tabBottombar .vb-block {
  flex-shrink: 1;
  text-align: center;
  padding: 16rpx 0 8rpx 0;
  flex: 1;
}
.tabBottombar .icon-img {
  width: 46rpx;
  height: 46rpx;
}
.tabBottombar .txt {
  font-size: 22rpx;
  line-height: 22rpx;
  color: #7f8389;
  margin-top: 5rpx;
}
.ac-list.media-list .item-title {
  font-size: 34rpx;
}
.ac-list.media-list .item-after {
  font-size: 32rpx;
  color: #999;
}
.ac-list.media-list .item-content {
  min-height: 100rpx;
}
.ac-list.media-list .item-inner {
  min-height: 100rpx;
}
.ac-list .item-media .img {
  width: 48rpx;
  height: 48rpx;
}
.hzo-service {
  background-color: #fff;
}
.hzo-service .tel {
  overflow: hidden;
  border-bottom: 1rpx solid rgba(221, 221, 221, 0.8);
}
.hzo-service .tel .gray-m {
  float: left;
  line-height: 60rpx;
}
.hzo-service .item {
  padding: 34rpx 32rpx;
}
.or-line .txt {
  width: 150rpx;
  margin-left: -75rpx;
}
