.container {
  padding-bottom: 100rpx;
}
.claim-process {
  background: url(https://image1.chinamuxie.com/images/common/hp-claim.png) no-repeat;
  background-origin: padding-box;
  background-size: 551rpx 1224rpx;
  background-position: 36rpx top;
  height: 1270rpx;
  box-sizing: border-box;
  background-color: #fff;
  position: relative;
}
.claim-process .tel-number {
  position: absolute;
  top: 95rpx;
  left: 240rpx;
  overflow: hidden;
  line-height: 60rpx;
}
.claim-process .tel-number .span,
.claim-process .tel-number .btn {
  float: left;
}
.hp-claim-btn {
  position: fixed;
  bottom: 0;
  text-align: center;
  background: #ff6a00;
  width: 750rpx;
  height: 98rpx;
  color: #fff;
  line-height: 98rpx;
  font-size: 36rpx;
}
