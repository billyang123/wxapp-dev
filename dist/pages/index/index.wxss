.tabBottombar {
  position: fixed;
  bottom: 0;
  left: 0;
  display: flex;
  background: #ffffff;
  width: 750rpx;
  height: 100rpx;
  border-top: 1rpx solid rgba(221, 221, 221, 0.8);
  z-index: 99999;
}
.tabBottombar .vb-block {
  flex-shrink: 1;
  text-align: center;
  padding: 16rpx 0 8rpx 0;
  flex: 1;
}
.tabBottombar .icon-img {
  width: 46rpx;
  height: 46rpx;
}
.tabBottombar .txt {
  font-size: 22rpx;
  line-height: 22rpx;
  color: #7f8389;
  margin-top: 5rpx;
}
.container {
  padding-bottom: 100rpx;
}
.swiper {
  margin-bottom: 30rpx;
  background-color: #fff;
  height: 250rpx;
}
.slide-image {
  height: 250rpx;
  width: 750rpx;
}
.hz-content {
  background-color: #fff;
  margin-bottom: 20rpx;
  border-bottom: 1rpx solid rgba(221, 221, 221, 0.8);
}
.hz-content .title {
  padding: 39rpx 36rpx 12rpx 36rpx;
}
.hz-content .title .tname {
  font-family: PingFangSC-Medium;
  border-left: 10rpx solid #ff6a00;
  padding-left: 18rpx;
  background-position: right 8rpx;
  background-repeat: no-repeat;
  background-image: url(https://image1.chinamuxie.com/images/common/index02_tit.png);
  background-size: 263rpx 240rpx;
}
.hz-content.hz-group .tname {
  background-position: right 8rpx;
}
.hz-content.hz-apply .tname {
  background-position: right -45rpx;
}
.hz-content.hzo-guarantee .tname {
  background-position: right -98rpx;
}
.hz-content.hz-dis .tname {
  background: none;
}
.hz-content.hzo-service .tname {
  background-position: right -202rpx;
}
.hz-content .hz-item {
  position: relative;
  padding: 40rpx 30rpx 40rpx 285rpx;
  border-bottom: 1rpx solid rgba(221, 221, 221, 0.5);
  height: 260rpx;
}
.hz-content .hz-item:last-child {
  border-bottom: none;
}
.hz-content .hz-item .hi-title {
  font-size: 36rpx;
  color: #333;
}
.hz-content .hz-item .hi-info {
  font-size: 28rpx;
  color: #999;
  line-height: 40rpx;
  margin: 25rpx 0;
  width: 400rpx;
  min-height: 100rpx;
}
.hz-content .hz-item .hi-info .org {
  color: #FF6A00;
}
.hz-content .hz-item .hi-opt {
  text-align: center;
  overflow: hidden;
}
.hz-content .hz-item .joinbtn {
  float: right;
  background: #ff6a00;
  border-radius: 4rpx;
  width: 160rpx;
  height: 60rpx;
  color: #fff;
  font-size: 30rpx;
  line-height: 60rpx;
  text-align: center;
  padding: 0;
}
.hz-content .hz-item .joinbtn:after {
  border-color: #ff6a00;
}
.hz-content .hz-item .opt-imglist {
  float: left;
  position: relative;
}
.hz-content .hz-item .opt-imglist .oimg {
  width: 56rpx;
  height: 56rpx;
  border-radius: 50%;
  position: absolute;
  border: 1px solid #fff;
}
.hz-content .hz-item .opt-imglist .oimg.img1 {
  top: 0;
  left: 0;
  z-index: 9;
}
.hz-content .hz-item .opt-imglist .oimg.img2 {
  top: 0;
  left: 45rpx;
  z-index: 8;
}
.hz-content .hz-item .opt-imglist .oimg.img3 {
  top: 0;
  left: 90rpx;
  z-index: 7;
}
.hz-content .hz-item .img {
  position: absolute;
  left: 35rpx;
  top: 40rpx;
  width: 220rpx;
  height: 260rpx;
}
.hz-process {
  padding: 40rpx 30rpx 40rpx 73rpx;
}
.hz-process .stepbar {
  border-radius: 50%;
  width: 64rpx;
  height: 64rpx;
  border: 1rpx solid #ff6a00;
  padding: 7rpx;
  position: absolute;
  top: 0;
  left: -40rpx;
  background-color: #fff;
}
.hz-process .stepbar .t-num {
  border-radius: 50%;
  width: 64rpx;
  height: 64rpx;
  line-height: 64rpx;
  font-size: 32rpx;
  background-color: #ff6a00;
  text-align: center;
  color: #fff;
}
.hz-process .step-item {
  position: relative;
  padding-left: 52rpx;
  border-left: 1px solid #ff6a00;
  padding-bottom: 46rpx;
  padding-top: 18rpx;
}
.hz-process .step-item:last-child {
  border-left: 1px solid transparent;
}
.hz-process .st-inner {
  font-size: 32rpx;
  color: #333;
  line-height: 40rpx;
}
.hz-process .st-inner.tel-kf {
  margin: 12rpx 0;
}
.hz-process .st-inner.tel-kf .gray {
  line-height: 60rpx;
  float: left;
}
.hz-process .st-inner .st {
  margin-right: 27rpx;
}
.hz-process .st-inner .gray {
  color: #999;
  font-size: 28rpx;
}
.hz-explain {
  padding: 40rpx 35rpx 50rpx 35rpx;
}
.hzo-guarantee .hzo-list {
  overflow: hidden;
  padding-top: 62rpx;
}
.hzo-guarantee .guarant-icon {
  width: 65rpx;
  height: 65rpx;
}
.hzo-guarantee .hzol-item {
  width: 375rpx;
  float: left;
  text-align: center;
  height: 400rpx;
}
.hzo-guarantee .hzol-item .ticon {
  font-size: 58rpx;
  color: #ff6a00;
}
.hzo-guarantee .hzol-item .tt {
  font-size: 36rpx;
  color: #333;
  margin: 25rpx 0 30rpx 0;
}
.hzo-guarantee .hzol-item .tt .iconfont {
  font-size: 36rpx;
  color: #ff6a00;
  margin-left: 7rpx;
}
.hzo-guarantee .hzol-item .tf {
  width: 296rpx;
  font-size: 28rpx;
  color: #999;
  text-align: left;
  margin: 0 auto;
}
.hzo-media .hz-media-list {
  padding: 36rpx;
}
.hzo-media .media {
  padding: 30rpx 36rpx 30rpx 150rpx;
  position: relative;
  height: 120rpx;
  border-bottom: 1rpx solid rgba(221, 221, 221, 0.5);
}
.hzo-media .media:last-child {
  border-bottom: 0;
}
.hzo-media .media .img {
  width: 120rpx;
  height: 120rpx;
  position: absolute;
  left: 0;
  top: 30rpx;
}
.hzo-media .media .mname {
  color: #333;
  font-size: 32rpx;
  line-height: 40rpx;
}
.hzo-media .media .time {
  color: #999;
  font-size: 26rpx;
  line-height: 40rpx;
}
.hzo-service {
  background-color: #fff;
}
.hzo-service .title {
  padding: 39rpx 36rpx 39rpx 36rpx;
  border-bottom: 1rpx solid rgba(221, 221, 221, 0.8);
}
.hzo-service .tel {
  overflow: hidden;
  border-bottom: 1rpx solid rgba(221, 221, 221, 0.8);
}
.hzo-service .tel .gray-m {
  float: left;
  line-height: 60rpx;
}
.hzo-service .item {
  padding: 34rpx 32rpx;
}
