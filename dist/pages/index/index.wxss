}
.stepbar {
  border-radius: 50%;
  width: 64rpx;
  height: 64rpx;
  border: 1rpx solid #ff6a00;
  padding: 7rpx;
  position: absolute;
  top: 0;
  left: -40rpx;
  background-color: #fff;
}
.t-num {
  border-radius: 50%;
  width: 64rpx;
  height: 64rpx;
  line-height: 64rpx;
  font-size: 32rpx;
  background-color: #ff6a00;
  text-align: center;
  color: #fff;
}
.step-item {
  position: relative;
  padding-left: 52rpx;
  border-left: 1px solid #ff6a00;
  padding-bottom: 46rpx;
  padding-top: 18rpx;
}
.step-item:last-child {
