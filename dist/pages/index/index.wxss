  height: 100rpx;
  border-top: 1rpx solid rgba(221, 221, 221, 0.8);
  z-index: 99999;
}
.vb-block {
  flex-shrink: 1;
  text-align: center;
  padding: 16rpx 0 8rpx 0;
  flex: 1;
}
.icon-img {
  width: 46rpx;
  height: 46rpx;
}
.vb-txt {
  font-size: 22rpx;
