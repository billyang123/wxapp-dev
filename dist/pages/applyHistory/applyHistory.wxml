<view class="container">
  <view class="applyHistory-no" xmlns:wx="" wx:if="{{apply}}">
    <view class="applyHistory"/>
    <view class="applyHistory-txt">
      <text>健健康康比什么都重要噢！</text>
    </view>
  </view>
  <view class="applyHistory-list" wx:else="wx:else">
    <view class="applyHistory-item" catchtap="_dispatch" data-link="/pages/applyDetail/applyDetail?type={{item.id}}" wx:for="{{array}}" wx:for-index="idx" data-catchtap="linkTo">
      <text class="applyHistory-item-txt">{{item.projectName}}</text>
      <text class="applyHistory-item-cont">{{item.claimContent}}</text>
      <text class="applyHistory-item-time">{{item.claimTimeStr}}</text>
    </view>
  </view>
  
</view>