.slide-list.media-list .icon-chevron {
  background: url("data:image/png;base64,") no-repeat;
  width: 15.6rpx;
  height: 27.6rpx;
  background-size: 15.6rpx 27.6rpx;
  transform: rotate(90deg);
}
.slide-list.media-list .item-title,
.slide-list.media-list .item-after {
  font-size: 32rpx;
}
.slide-list.media-list .item-title {
  color: #666;
}
.slide-list.media-list .item-after {
  color: #c7c7c7;
}
.slide-list.media-list .item-content {
  padding-left: 25rpx;
  padding-right: 25rpx;
}
.slide-list.media-list .item-inner {
  position: relative;
  padding-right: 35rpx;
}
.slide-list.media-list .item-inner .icon-chevron {
  position: absolute;
  right: 9rpx;
  top: 38rpx;
}
.slide-list.media-list .item-xmain {
  background-color: #f7f8fa;
  font-size: 32rpx;
  line-height: 58rpx;
  overflow: hidden;
}
.slide-list.media-list .item-xmain .p {
  text-indent: 56rpx;
  color: #666;
}
.slide-list.media-list .item-xmain-bd {
  padding: 25rpx;
}
.slide-list.media-list .item-xmain.auto {
  height: auto !important;
}
